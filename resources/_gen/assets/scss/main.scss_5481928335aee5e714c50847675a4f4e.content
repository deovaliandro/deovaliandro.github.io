@import "https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap";@import "https://fonts.googleapis.com/css2?family=JetBrains+Mono:ital,wght@0,100..800;1,100..800&display=swap";:root{text-rendering:geometricPrecision}body{font-family:inter,sans-serif;font-optical-sizing:auto;font-style:normal;font-variation-settings:"slnt" 0;font-size:13;line-height:1.6;margin-inline:auto;width:800px;word-wrap:break-word;overflow-wrap:break-word;overflow-y:scroll}a{color:#222;text-decoration:none}a:not(.list-post>a,.footnote-ref){border-bottom:1px dashed gray}.list-post{border-bottom:1px dashed gray}.list-post>a{color:#222}.list-post:hover{background:#24292e;color:#fff;background:#24292e;color:#fff}.list-post:hover a{background:#24292e;color:#fff}.title{border-bottom:1px dashed gray}.time-post{float:right;font-family:jetbrains mono,monospace}img{max-width:100%;display:block;margin-inline:auto}pre,code{font-family:jetbrains mono,monospace}code{padding:2px 5px;background-color:#f2f2f2}pre code{color:#222;display:block;padding:20px;white-space:pre-wrap;overflow-x:auto}blockquote{border-left:4px solid grey;padding:5px 0 5px 20px}footer{padding:25px;text-align:center}footer a{border-bottom:1px dashed gray}ul.blog-posts{list-style-type:none;padding:unset}.notfound{text-align:center}code:not(pre>code){padding:1px 2px;border-radius:1ch;border:1px solid #f3f3f3}table{border-collapse:collapse;margin:1.5rem 0}figure>table{width:max-content}td{border:1px solid #898ea4;text-align:start;padding:.5rem}th{border:1px solid #898ea4;text-align:start;padding:.5rem;background-color:#f5f7ff;font-weight:700}tr:nth-child(even){background-color:#f5f7ff}table caption{font-weight:700;margin-bottom:.5rem}hr{border:none;height:1px;background:#898ea4;margin:1rem auto}kbd{background-color:rgba(200,200,200,.15);border:1px solid rgba(0,0,0,.3);border-radius:.2em;box-shadow:inset 0 1px rgba(255,255,255,.12),inset 0 -2px rgba(0,0,0,.15);display:inline-block;line-height:1.5;margin:0 .15em;min-width:1em;padding:.125em .5em;text-align:center;user-select:none;white-space:nowrap}mark{padding:2px 5px;border-radius:1ch;border:1px solid #f0d547;background-color:#fff8d3}mark a{color:#0d47a1}.footnote-ref{padding:2px 5px;border-radius:1ch;border:1px solid #f0d547;background-color:#fff8d3}